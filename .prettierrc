{
	"useTabs": true,
	"singleQuote": true,
	"quoteProps": "consistent",
	"trailingComma": "all",
	"printWidth": 100,
	"overrides": [
		{ "files": "*.jsonc", "options": { "trailingComma": "none" } },
		{ "files": "*.md", "options": { "useTabs": false, "tabWidth": 2 } },
		{ "files": "tsconfig*.json", "options": { "parser": "jsonc" } }
	]
}
